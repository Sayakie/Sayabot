        "nodemon": "^1.18.10",
        "prettier": "^1.16.4",
        "ts-node": "^8.0.2",
        "tsconfig-paths": "^3.8.0",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "^3.3.3"
