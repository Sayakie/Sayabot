{
    "private": true,
    "name": "sayabot",
    "version": "0.1.0",
    "description": "Simplest way to start Discord bot is using Sayabot",
    "engines": {
        "node": ">= 8"
    },
    "homepage": "https://github.com/Sayakie/Sayabot#readme",
    "bugs": {
        "url": "https://github.com/Sayakie/Sayabot/issues"
    },
    "author": "Sayakie <shuvi.dev.b701@gmail.com>",
